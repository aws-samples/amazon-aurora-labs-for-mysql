
global:
  owner: {{ config.owner_email }}
  qsname: {{ config.project_name }}
  regions:
  {% for region in config.supported_regions -%}
    - {{ region }}
  {% endfor -%}
  reporting: true
tests:
  taskcat-json:
    parameter_input: debug-input.json
    template_file: debug.template
    regions:
      - us-east-1
  taskcat-yaml:
    parameter_input: debug-input.json
    template_file: debug-yaml.template
    regions:
      - us-east-1
